{
  "name": "kombucha-shop-redux",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "redux": "^4.1.0",
    "styled-components": "^5.2.3",
    "uuid": "^8.3.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "semi": [
        "error",
        "always"
      ],
      "semi-spacing": "error",
      "no-multiple-empty-lines": "error",
      "no-unused-vars": "warn",
      "keyword-spacing": "error",
      "space-before-function-paren": "error",
      "object-curly-spacing": [
        "warn",
        "always"
      ],
      "react/react-in-jsx-scope": "off",
      "eqeqeq": "error",
      "curly": "error",
      "quotes": [
        "error",
        "single"
      ],
      "block-spacing": "error",
      "arrow-parens": [
        "error",
        "as-needed"
      ],
      "brace-style": [
        "error",
        "1tbs"
      ],
      "no-trailing-spaces": [
        "error",
        {
          "skipBlankLines": true,
          "ignoreComments": true
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
